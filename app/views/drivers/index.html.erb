			<!-- お気に入り画面 -->
			<div role="tabpanel" class="tab-pane fade" id="fav">
				<!-- HEADER(( -->
				<div class="wrap header">
					<div class="navbar-fixed-top">
						<div class="container　center-block" style="background-color: black;">
							<div class="row">
								<div class="logo col-xs-4 col-sm-4 left-block">
                  <%= image_tag("logo.png") %>
								</div>
								<div class="col-xs-4 col-sm-4 center-block" role="navigation" style="display: inline-block;">
									<h1>Favorite</h1>
								</div>
							</div>
						</div>
					</div>
				</div>
				<!-- HEADER)) -->
				<!-- BODY(( -->
        <table class="table table-bordered countents" style="margin-top: 69px">
					<thead style="color: white;">
						<tr>
							<th>Favorite Driver</th>
						</tr>
            </thead>
          <tbody>
            <% if !@fav_drivers.nil? %>
              <% @fav_drivers.each do |driver| %>
                <tr class="warning">
                  <td>
                    <div class="row drivers">
                      <%= image_tag('person.png', :class => 'col-xs-4 col-sm-4 left-block') %>
                      <h1 class="name col-xs-8 col-sm-8 right-block"><%= driver.name %></h1>
                      <h2 class="career col-xs-4 col-sm-4 left-block"><%= driver.place %></h2>
                      <h2 class="evaluation col-xs-4 col-sm-4 right-block"><%= driver.review %>/5</h2>
                      <div class="text-center">
                        <div class="col-xs-4 col-sm-4 left-block"></div>
                        <a data-method="post" href="/drivers/<%= driver.id %>/history" rel="nofollow"><div type="button" class="btn btn-info col-xs-4 col-sm-4 text-center"><div class="glyphicon glyphicon-earphone"></div></div></a>
                      </div>
                      <%= link_to 'Unfavorite', driver_favorite_path(driver.id), method: :delete %>
                  </td>
                </tr>
              <% end %>
            <% end %>
					</tbody>
				</table>
			</div>
			<!-- ホーム画面-->
			<div role="tabpanel" class="tab-pane fade" id="home">
				<!-- HEADER(( -->
				<div class="wrap header">
					<div class="navbar-fixed-top">
						<div class="container　center-block" style="background-color: black;">
							<div class="row">
								<div class="logo col-xs-4 col-sm-4 left-block">
                  <%= image_tag("logo.png") %>
								</div>
								<div class="col-xs-4 col-sm-4 center-block" role="navigation" style="display: inline-block;">
									<h1>Home</h1>
								</div>
							</div>
						</div>
					</div>
				</div>
				<!-- HEADER)) -->
				<!-- BODY(( -->
				<table class="table table-bordered countents" style="margin-top: 69px">
					<thead style="color: white;">
						<tr>
							<th>History</th>
						</tr>
          </thead>
					<tbody>
            <% if !@his_drivers.nil? %>
              <% @his_drivers.each do |driver| %>
                <tr class="warning">
                  <td>
                    <div class="row drivers">
                      <%= image_tag('person.png', :class => 'col-xs-4 col-sm-4 left-block') %>
                      <h1 class="name col-xs-8 col-sm-8 right-block"><%= driver.name %></h1>
                      <h2 class="career col-xs-8 col-sm-8 right-block"><%= driver.place %></h2>
                      <div class="text-center">
                        <!-- 運転手の評価 -->
                        <div class="col-xs-1 col-sm-1"></div>
                        <div class="col-xs-2 col-sm-2 left-block"><p>Review</p></div>
                        <div class="col-xs-4 col-sm-4 left-block">
                          <select class="form-control">
                            <option value="1"<%= " selected" if driver.review==1 %>>1</option>
                            <option value="2"<%= " selected" if driver.review==2 %>>2</option>
                            <option value="3"<%= " selected" if driver.review==3 %>>3</option>
                            <option value="4"<%= " selected" if driver.review==4 %>>4</option>
                            <option value="5"<%= " selected" if driver.review==5 %>>5</option>
                          </select>
                        </div>
                        <div class="col-xs-1 col-sm-1"></div>
                        <% if driver.favorited_by? current_user %>
                          <a data-method="delete" href="/drivers/<%= driver.id %>/favorite" rel="nofollow"><div type="button" class="btn btn-info col-xs-4 col-sm-4">Unfavorite</div></div></a>
                        <% else %>
                          <a data-method="post" href="/drivers/<%= driver.id %>/favorite" rel="nofollow"><div type="button" class="btn btn-info col-xs-4 col-sm-4"><div class="glyphicon glyphicon-star-empty"></div></div></a>
                        <% end %>
                      </div>
                    </div>
                  </td>
                </tr>
              <% end %>
            <% end %>
					</tbody>
				</table>
			</div>
		</div>
	</div>

